<TreeGrid xmlns="flexlib.controls.*"
          xmlns:fx="http://ns.adobe.com/mxml/2009"
          xmlns:mx="library://ns.adobe.com/flex/mx"
          headerStyleName="dataGridHeader"
          verticalTrunks="none"
          sortableColumns="false"
          editable="false"
          rowHeight="17"
          selectable="true"
          xmlns:treeGridClasses="flexlib.controls.treeGridClasses.*"
          >
<columns>
        <treeGridClasses:TreeGridColumn dataField="name"
                                        headerText="name"
                                        minWidth="50"
                                        itemRenderer="org.puremvc.as3.multicore.utilities.fabrication.console.view.components.base.inspectdatagrid.itemrenderers.TGItemRenderer"/>
        <mx:DataGridColumn dataField="type"
                           headerText="type"
                           width="50" minWidth="50"
                           itemRenderer="org.puremvc.as3.multicore.utilities.fabrication.console.view.components.base.inspectdatagrid.itemrenderers.DGItemRenderer"/>
        <mx:DataGridColumn dataField="value"
                           headerText="value"
                           width="200"
                           wordWrap="true"
                           showDataTips="true"
                           itemRenderer="org.puremvc.as3.multicore.utilities.fabrication.console.view.components.base.inspectdatagrid.itemrenderers.DGItemRenderer"
                           
                />
    </columns>
</TreeGrid>
