<fabrication:FlexModule
        xmlns:fx="http://ns.adobe.com/mxml/2009"
        xmlns:s="library://ns.adobe.com/flex/spark"
        xmlns:fabrication="http://puremvc.org/utilities/fabrication/2010"
        width="100" height="100">
    <fabrication:layout>
        <s:BasicLayout />
    </fabrication:layout>
    <fx:Script>
		<![CDATA[
        import simplemodule.controller.SimpleModuleStartupCommand;

        override public function getStartupCommand():Class
        {
            return SimpleModuleStartupCommand;
        }

        override public function getClassByName(path:String):Class
        {
            return getDefinitionByName(path) as Class;
        }
			
		]]>
	</fx:Script>
    <s:Group  width="100%" height="100%">
        <s:Rect width="100%" height="100%">
            <s:fill>
                <s:SolidColor color="0x000000"/>
            </s:fill>
        </s:Rect>
    </s:Group>
    <s:Label text="module" color="0xFFFFF" horizontalCenter="0" verticalCenter="0"/>

</fabrication:FlexModule>
